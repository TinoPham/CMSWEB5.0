@model IEnumerable<SVRDatabase.Model.Log>

@{
    ViewBag.Title = "Logs List";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Logs List</h2>

<p>
    @Html.ActionLink("Create New", "Create",null,new{@class = "btn btn-warning"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LogID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th>
            Message Data
        </th>
        <th>
            Program Set
        </th>
        <th>
            DVR Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LogID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MessageData)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProgramSet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DVRDate)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
